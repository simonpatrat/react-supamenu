{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "jsx": "react-jsx",
    "sourceMap": true,
    "lib": ["dom", "esnext"],
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "*.js", "dist"]
}
